{
  "compilerOptions": {
    "target": "es5",
		"module": "commonjs",
		"sourceMap": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true
  },
  "typeRoots": ["./node_modules/@types", "./node_modules/phaser-ce/typescript/"],
  "files": [
    "src/app.ts",
  ],
  "include": [
    "src/**/*"
  ],
  "exclude": ["node_modules", "*.spec.ts"],
  "awesomeTypescriptLoaderOptions": {
    "transpileOnly": true
  }
}
